                        <term_definitions code="at0000">
                          <items id="description">Details of a medicine, vaccine or other therapeutic good with instructions for use.</items>
                          <items id="text">Medication order</items>
                        </term_definitions>
                        <term_definitions code="at0001">
                          <items id="description">Order.</items>
                          <items id="text">Order</items>
                        </term_definitions>
                        <term_definitions code="at0002">
                          <items id="description">@ internal @</items>
                          <items id="text">Tree</items>
                        </term_definitions>
                        <term_definitions code="at0004">
                          <items id="description">Unique identifier for the medication order.</items>
                          <items id="text">Order identifier</items>
                        </term_definitions>
                        <term_definitions code="at0005">
                          <items id="description">The amount and units of the medicine, vaccine or other therapeutic good to be used or administered at one time.</items>
                          <items id="text">Dose amount description</items>
                        </term_definitions>
                        <term_definitions code="at0008">
                          <items id="comment">For example: local information requirements or additional metadata to align with FHIR or CIMI equivalents.</items>
                          <items id="description">Additional information required to capture local content or to align with other reference models/formalisms.</items>
                          <items id="text">Extension</items>
                        </term_definitions>
                        <term_definitions code="at0009">
                          <items id="comment">Where the medication dose directions are fully carried by the structured, computable dose directions, this element should carry the narrative equivalent, generally auto-generated. Where it is not possible to represent the intended dosage directions fully in computable form, these should only be carried here only in narrative form. </items>
                          <items id="description">A complete narrative description of how much, when and how to use the medicine, vaccine or other therapeutic good.</items>
                          <items id="text">Dose directions description</items>
                        </term_definitions>
                        <term_definitions code="at0011">
                          <items id="comment">e.g. 'Start if symptoms recur'.</items>
                          <items id="description">A condition which, when met, requires the start of administration or use.</items>
                          <items id="text">Order start criterion</items>
                        </term_definitions>
                        <term_definitions code="at0012">
                          <items id="description">The date and optional time to begin using the medicine, vaccine or other therapeutic good.</items>
                          <items id="text">Order start date/time</items>
                        </term_definitions>
                        <term_definitions code="at0013">
                          <items id="description">The date and optional time to stop using the medicine, vaccine or other therapeutic good.</items>
                          <items id="text">Order stop date/time</items>
                        </term_definitions>
                        <term_definitions code="at0016">
                          <items id="comment">e.g. 'Stop after symptoms disappear''.</items>
                          <items id="description">A condition which, when met, requires the cessation of administration or use.</items>
                          <items id="text">Order stop criterion</items>
                        </term_definitions>
                        <term_definitions code="at0018">
                          <items id="comment">e.g. 'Angina'.</items>
                          <items id="description">A reason for ordering the medicine, vaccine or other therapeutic good.</items>
                          <items id="text">Clinical indication</items>
                        </term_definitions>
                        <term_definitions code="at0037">
                          <items id="comment">e.g. 'in the morning', 'at 0600 1400 2100'.</items>
                          <items id="description">Structured details of the timing of a single use or administration.</items>
                          <items id="text">Dose timing</items>
                        </term_definitions>
                        <term_definitions code="at0043">
                          <items id="description">Further structured details of the administration of the medicine, vaccine or other therapeutic good.</items>
                          <items id="text">Administration details</items>
                        </term_definitions>
                        <term_definitions code="at0044">
                          <items id="comment">e.g. 'take with food'</items>
                          <items id="description">An additional statement on how to use the medicine, vaccine or other therapeutic good.</items>
                          <items id="text">Additional instruction</items>
                        </term_definitions>
                        <term_definitions code="at0047">
                          <items id="comment">e.g. '10mg bd; 20mg n' as used by the NHS Dose syntax ( in development).</items>
                          <items id="description">A structured, parsable and computable representation of the dose directions. Generally only used when passing information between legacy systems.</items>
                          <items id="text">Parsable dose directions</items>
                        </term_definitions>
                        <term_definitions code="at0050">
                          <items id="comment">e.g. To record that the patient had been taking antibiotics three days prior to hospital admission, in the context of a 7-day course.</items>
                          <items id="description">The time period for which the patient was already on this  treatment prior to this instruction being issued.</items>
                          <items id="text">Duration of course already complete</items>
                        </term_definitions>
                        <term_definitions code="at0051">
                          <items id="comment">e.g. 'up to 1gram per day'.
Multiple maximum doses may be specified for different time periods.</items>
                          <items id="description">Details of maximum dose allowed over a defined period.</items>
                          <items id="text">Maximum dose</items>
                        </term_definitions>
                        <term_definitions code="at0053">
                          <items id="description">The allowed period for the maximal dose.</items>
                          <items id="text">Allowed period</items>
                        </term_definitions>
                        <term_definitions code="at0055">
                          <items id="comment">Will generally only be used for legacy formats where dose directions are simply split between textual representations of dose and amount.</items>
                          <items id="description">A narrative description of the dose frequency and other timing of the medicine, vaccine or other therapeutic good.</items>
                          <items id="text">Dose timing description</items>
                        </term_definitions>
                        <term_definitions code="at0056">
                          <items id="comment">e.g. '1 tab in the morning 1 tab at night for 3 weeks, on Mondays Wednesdays and Fridays'.</items>
                          <items id="description">One or more structured directions, including structured dose amount and timing and making up the complete set of directions for the medication order.</items>
                          <items id="text">Dose direction</items>
                        </term_definitions>
                        <term_definitions code="at0057">
                          <items id="description">The intended order of this direction within the overall sequence.</items>
                          <items id="text">Direction sequence</items>
                        </term_definitions>
                        <term_definitions code="at0058">
                          <items id="comment">e.g. '2 tablets at 6pm' or '20mg three times per day'.</items>
                          <items id="description">The combination of a medication amount associated with a single medication timing.</items>
                          <items id="text">Dose pattern</items>
                        </term_definitions>
                        <term_definitions code="at0060">
                          <items id="comment">e.g. To record that the patient had been taking antibiotics three days prior to hospital admission, in the context of a 7-day course.</items>
                          <items id="description">The number of administrations of the medicine, vaccine or other therapeutic good that have been administered as part of the overall course but prior to this instruction.</items>
                          <items id="text">Administrations already complete</items>
                        </term_definitions>
                        <term_definitions code="at0062">
                          <items id="description">Medication safety details of the medicine, vaccine or other therapeutic good.</items>
                          <items id="text">Medication safety</items>
                        </term_definitions>
                        <term_definitions code="at0064">
                          <items id="description">Does the prescriber confirm that the normal dose has been overriden in exceptional circumstances?</items>
                          <items id="text">Exceptional dose override</items>
                        </term_definitions>
                        <term_definitions code="at0065">
                          <items id="description">Further structured details of dispensing of the medicine, vaccine or other therapeutic good.</items>
                          <items id="text">Dispensing details</items>
                        </term_definitions>
                        <term_definitions code="at0066">
                          <items id="comment">e.g. 'for 7 days','Indefinite'.</items>
                          <items id="description">The duration for which this direction should be continued.</items>
                          <items id="text">Direction duration</items>
                        </term_definitions>
                        <term_definitions code="at0067">
                          <items id="description">The direction should be continued indefinitely.</items>
                          <items id="text">Indefinite</items>
                        </term_definitions>
                        <term_definitions code="at0068">
                          <items id="description">The direction should be continued indefinitely and discontinuation is not recommended.</items>
                          <items id="text">Indefinite - Do not discontinue</items>
                        </term_definitions>
                        <term_definitions code="at0069">
                          <items id="comment">Medicine authorisation and re-authorisation requirements vary in different jurisdictions.</items>
                          <items id="description">Details of authorisation of the medicine, vaccine or other therapeutic good, including details of repeat supply.</items>
                          <items id="text">Authorisation details</items>
                        </term_definitions>
                        <term_definitions code="at0070">
                          <items id="comment">e.g. 'Atenolol'.</items>
                          <items id="description">The medicine, vaccine or other therapeutic good being ordered, administered to or used by the subject of care. This item should be coded if possible.</items>
                          <items id="text">Medication</items>
                        </term_definitions>
                        <term_definitions code="at0090">
                          <items id="comment">e.g. 'every 3 days', 'on Thursdays and Sundays', '3 days after onset of menstruation'.</items>
                          <items id="description">Details of the days on which each set of dose directions should be applied.</items>
                          <items id="text">Direction repetition</items>
                        </term_definitions>
                        <term_definitions code="at0091">
                          <items id="comment">e.g. 'oral', íntravenous', 'topical'.</items>
                          <items id="description">The route of administration.</items>
                          <items id="text">Route</items>
                        </term_definitions>
                        <term_definitions code="at0092">
                          <items id="comment">e.g. 'left upper arm', 'intravenous catheter right hand'.</items>
                          <items id="description">The anatomical site of administration.</items>
                          <items id="text">Site</items>
                        </term_definitions>
                        <term_definitions code="at0093">
                          <items id="description">Detailed anatomical site of administration.</items>
                          <items id="text">Structured body site</items>
                        </term_definitions>
                        <term_definitions code="at0094">
                          <items id="comment">e.g. 'via nebuliser'.</items>
                          <items id="description">The method by which the medicine is to be delivered.</items>
                          <items id="text">Delivery method</items>
                        </term_definitions>
                        <term_definitions code="at0095">
                          <items id="description">Details of any device used to aid delivery of the medicine.</items>
                          <items id="text">Delivery device</items>
                        </term_definitions>
                        <term_definitions code="at0102">
                          <items id="comment">e.g. 'Administer over 10 minutes'.</items>
                          <items id="description">The period of time over which a single dose of the medicine  should be administered.</items>
                          <items id="text">Dose administration duration</items>
                        </term_definitions>
                        <term_definitions code="at0105">
                          <items id="description">An additional instruction directed primarily at the patient.</items>
                          <items id="text">Patient instruction</items>
                        </term_definitions>
                        <term_definitions code="at0106">
                          <items id="description">An additional instruction directed at the dispenser.</items>
                          <items id="text">Dispensing instruction</items>
                        </term_definitions>
                        <term_definitions code="at0107">
                          <items id="comment">e.g. 'Please check renal function in 2 weeks'.</items>
                          <items id="description">An additional instruction which gives advice on appropriate monitoring of the medication e.g. blood testing.</items>
                          <items id="text">Monitoring instruction</items>
                        </term_definitions>
                        <term_definitions code="at0108">
                          <items id="description">An additional instruction directed primarily at the person administering the medication.</items>
                          <items id="text">Administration instruction</items>
                        </term_definitions>
                        <term_definitions code="at0109">
                          <items id="comment">Will generally only be used for legacy formats where dose directions are simply split between textual representations of dose and amount.</items>
                          <items id="description">A narrative description of the dose amount of the medicine, vaccine or other therapeutic good.</items>
                          <items id="text">Dose amount description</items>
                        </term_definitions>
                        <term_definitions code="at0112">
                          <items id="comment">Course summary is required when transmitting medication information between systems e.g as part of a referral, emergency patient summary or discharge communication.</items>
                          <items id="description">Summary information about the medication, such as current status or key dates, generally used in non-prescription contexts.</items>
                          <items id="text">Course summary</items>
                        </term_definitions>
                        <term_definitions code="at0113">
                          <items id="description">Details of the overall course of the medication.</items>
                          <items id="text">Course details</items>
                        </term_definitions>
                        <term_definitions code="at0127">
                          <items id="description">The maximum dose allowed over a lifetime.</items>
                          <items id="text">Cumulative maximum dose</items>
                        </term_definitions>
                        <term_definitions code="at0129">
                          <items id="description">Details of the order pertinent to the dispenser.</items>
                          <items id="text">Dispensing</items>
                        </term_definitions>
                        <term_definitions code="at0130">
                          <items id="comment"> e.g 1, 1.5, 0.125</items>
                          <items id="description">The maximum amount of medication allowed in the defined period.</items>
                          <items id="text">Maximum amount</items>
                        </term_definitions>
                        <term_definitions code="at0131">
                          <items id="comment"> e.g 1, 1.5, 0.125</items>
                          <items id="description">The amount of medication to dispense.</items>
                          <items id="text">Amount to dispense</items>
                        </term_definitions>
                        <term_definitions code="at0132">
                          <items id="description">Indicates whether or not the substitution of a prescribed medicine with a different brand name of the same medicine, vaccine or other therapeutic good, which has been determined as bioequivalent, is allowed when the medication is dispensed/supplied.</items>
                          <items id="text">Substitution permitted</items>
                        </term_definitions>
                        <term_definitions code="at0133">
                          <items id="description">Details pertinent to administration of the medication.</items>
                          <items id="text">Administration</items>
                        </term_definitions>
                        <term_definitions code="at0134">
                          <items id="comment">Use the Text choice to record non or semi-quantifiable instructions e.g. 1 drop per minute.</items>
                          <items id="description">The rate at which the infusion is to be administered.</items>
                          <items id="text">Infusion administration rate</items>
                        </term_definitions>
                        <term_definitions code="at0135">
                          <items id="description">The formula used to calculate the Dose amount or administration rate where this is dependent on some other factor such as patient weight e.g. 10mg/kg/day.</items>
                          <items id="text">Dosage formula</items>
                        </term_definitions>
                        <term_definitions code="at0136">
                          <items id="description">The purpose of the infusion.</items>
                          <items id="text">Infusion purpose</items>
                        </term_definitions>
                        <term_definitions code="at0137">
                          <items id="description">The infusion provides baseline hydration.</items>
                          <items id="text">Baseline electrolyte infusion</items>
                        </term_definitions>
                        <term_definitions code="at0138">
                          <items id="description">The infusion carries an active pharrmaceutical ingredient.</items>
                          <items id="text">Active medication infusion</items>
                        </term_definitions>
                        <term_definitions code="at0139">
                          <items id="description">An indicator for the urgency with which the medication should be dispensed.</items>
                          <items id="text">Priority</items>
                        </term_definitions>
                        <term_definitions code="at0140">
                          <items id="comment">'30 tablets'.</items>
                          <items id="description">The amount of medication dispensed expressed in narrative form where this cannot be handled by a structured expression.</items>
                          <items id="text">Dispensing supply description</items>
                        </term_definitions>
                        <term_definitions code="at0141">
                          <items id="description">Details of the amount of medication to be supplied by the dispenser.</items>
                          <items id="text">Quantity to dispense</items>
                        </term_definitions>
                        <term_definitions code="at0142">
                          <items id="description">The dispenser is asked to supply sufficient quantity of medication to cover the defined period.</items>
                          <items id="text">Duration to supply</items>
                        </term_definitions>
                        <term_definitions code="at0143">
                          <items id="description">The strength and form of the overall preparation, including form, strength, any diluents or mixture of ingredients.</items>
                          <items id="text">Preparation</items>
                        </term_definitions>
                        <term_definitions code="at0144">
                          <items id="comment"> e.g 1, 1.5, 0.125 or 1-2, 12.5-20.5</items>
                          <items id="description">The value of the amount of medication as a real number.</items>
                          <items id="text">Dose amount</items>
                        </term_definitions>
                        <term_definitions code="at0145">
                          <items id="comment">e.g 'tablet','mg'. Often coded using a reference terminology such as SNOMED CT.</items>
                          <items id="description">The dose unit of the amount of medication.</items>
                          <items id="text">Dose unit</items>
                        </term_definitions>
                        <term_definitions code="at0146">
                          <items id="comment">e.g 'tablet','mg'. Often coded using a reference terminology such as SNOMED CT.</items>
                          <items id="description">The dose unit for the maxumum amount of medication allowed.</items>
                          <items id="text">Maximum amount dose unit</items>
                        </term_definitions>
                        <term_definitions code="at0147">
                          <items id="comment">e.g.'tablets', 'packs', ml'.</items>
                          <items id="description">The dose unit or pack unit associated with the dispense amount.</items>
                          <items id="text">Dispense unit</items>
                        </term_definitions>
                        <term_definitions code="at0148">
                          <items id="comment">e.g. 'palliative', 'low-dose propylaxis'.</items>
                          <items id="description">The overall theraputic intent of the medication.</items>
                          <items id="text">Therapeutic intent</items>
                        </term_definitions>
                      </children>
