						defining_code matches {
							[local::
							at0023, 	-- Left
							at0024]	-- Right
						}
					}
				}
			}
			ELEMENT[at0001] occurrences matches {0..1} matches {	-- Clinical Description
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0002] occurrences matches {0..1} matches {	-- Description of Skin
				value matches {
					DV_TEXT matches {*}
				}
			}
